<bg xmlns:xsi='http://www.w3.org/2001/XMLSchema-instance' xsi:schemaLocation='https://monkeymanboy.github.io/BSML-Docs/ https://raw.githubusercontent.com/monkeymanboy/BSML-Docs/gh-pages/BSMLSchema.xsd'>
<modifier-container child-control-width='true' child-expand-width='true'>
  
  <modifier value='hiddenToggle' on-change='setHiddenToggle' source='HiddenBlocks.Resources.HiddenIcon.png' text='Enabled'></modifier>
 <increment-setting formatter='distanceFormatter' value='hiddenDistance' on-change='setHideDistance' text='Hide' increment='0.1' min='4.5' max='10' integer-only='false'></increment-setting>
</modifier-container>
</bg>